(define (cube x) (* x x x))
(define (cube-good-enough? guess x)
	(< (abs (- (cube guess) x)) 0.001))
(define (cube-improve guess x)
	(/ (+ (/ x (square guess)) (* 2 guess)) 3)
)
(define (cube-iter guess x)
	(if (cube-good-enough? guess x)
	guess
	(cube-iter (cube-improve guess x) x)))
(define (crt x)
  (cube-iter 1.0 x))
